{
  "name": "node-frontapp",
  "version": "0.1.7",
  "description": "Node FrontApp API Wrapper in Typescript",
  "homepage": "https://github.com/vsiguero/node-frontapp",
  "bugs:": "https://github.com/vsiguero/node-frontapp/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/vsiguero/node-frontapp.git"
  },
  "author": {
    "name": "VÃ­ctor Siguero",
    "email": "vsiguero@gmail.com"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    "frontapp",
    "api",
    "typescript",
    "node"
  ],
  "dependencies": {
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/prompt-sync": "^4.1.0",
    "@types/superagent": "^4.1.7",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.2.7",
    "json-loader": "^0.5.7",
    "nock": "^12.0.3",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "tsc && node_modules/.bin/webpack --config webpack.config.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles"
  },
  "engines": {
    "node": ">=8.0.0 <13.0.0"
  },
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
